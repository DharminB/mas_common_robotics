<?xml version="1.0"?>
<launch>
	<node pkg="mcr_body_detection_3d" type="body_detection_3d_node" name="mcr_body_detection_3d" output="screen">
		<param name="model_filename" value="$(find mcr_body_detection_3d)/ros/config/body_detection_model.mdl" />
	</node>

	<test pkg="mcr_body_detection_3d" type="integration_test" test-name="mcr_body_detection_3d_integration_test">
			<param name="dataset_path" value="$(find mds_pointclouds)/people/single_persons/" />
			<param name="component_namespace" value="/mcr_body_detection_3d" />
	</test>

</launch>
