<?xml version="1.0"?>
<launch>

	<node pkg="mcr_door_status" type="door_status_node" name="mcr_door_status" ns="/mcr_perception" output="screen">
		<param name="angular_range" value="12.0" />	 <!-- in degree -->
		<param name="distance_threshold" value="1.2" />  <!-- in meter -->

		<remap from="scan" to="/scan_front"/>
		<remap from="door_status" to="door_status"/>

	</node>
</launch>
