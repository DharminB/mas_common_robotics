<?xml version="1.0"?>
<launch>
   
    <!-- start gazebo with empty world -->
	<include file="$(find mcr_gazebo_worlds)/ros/launch/empty-world.launch" />

    <!-- send world urdf to param server -->
	<param name="arena_description" command="$(find xacro)/xacro.py $(find mcr_gazebo_worlds)/common/worlds/$(env ROBOT_ENV).urdf.xacro" />

    <!-- spawn uploaded world model -->
	<node name="gazebo_arena_model" pkg="gazebo" type="spawn_model" args="-urdf -param arena_description -model world -x 0.0 -y 0.0 -z 0.2" respawn="false" output="screen" />    

</launch>



