<?xml version="1.0"?>
<package>
  <name>mcr_online_jshop2</name>
  <version>0.1.0</version>
  <description>
    Library to use JSHOP2 online i.e. the domain-dependent planner is generated
    and compiled in memory.
  </description>
  <license>GPLv3</license>

  <maintainer email="sven.schneider@h-brs.de">Sven Schneider</maintainer>
  <author email="sven.schneider@h-brs.de">Sven Schneider</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rosjava_core</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>mcr_gradlew</build_depend>
  <build_depend>mcr_jshop2</build_depend>
  <build_depend>mcr_java_messages</build_depend>
  <build_depend>mcr_task_planning_msgs</build_depend>
  <build_depend>rostest</build_depend>

  <run_depend>mcr_jshop2</run_depend>
  <run_depend>mcr_java_messages</run_depend>
</package>
